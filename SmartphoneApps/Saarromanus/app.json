{
  "expo": {
    "name": "Saarromanus",
    "slug": "Saarromanus",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
	},
	"android": {
		"package": "com.se.saarromanus",
		"config": {
			"googleMaps": {
				"apiKey": "YOUR_NEWLY_CREATED_API_KEY_WILL_GO_HERE"
			}
		}
	}
  }
}
